{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "id-length": 0,
    "react/no-did-mount-set-state": 0,
    "no-underscore-dangle": 0,
    "global-require": 0,
    "quotes": 0,

    "import/no-unresolved": 0,
    "max-len": 0,
    "react/prefer-stateless-function": 0,
    "prefer-template": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "template-curly-spacing": 0,
    "no-unneeded-ternary": 0,
    "newline-per-chained-call": 0,
    "no-unused-vars": 0,
  },
}
